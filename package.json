    "lint:style": "stylelint \"**/*.css\"",
    "test": "npm run lint && npm run jest && npm run lint:style",
    "dev": "concurrently \"npm run start\" \"cd ../bams_barber_shop && npm run dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
